{
  "publisher": "lbauskar",
  "name": "kanban",
  "displayName": "Kanban",
  "description": "Simple kanban board for VS Code. Visually organize your ideas!",
  "icon": "images/icon.png",
  "version": "1.7.0",
  "engines": {
    "vscode": "^1.53.2"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "categories": [
    "Visualization",
    "Notebooks"
  ],
  "keywords": [
    "kanban",
    "board",
    "agile",
    "project",
    "management",
    "sprint",
    "productivity",
    "scrum"
  ],
  "extensionKind": [
    "ui"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./build/extension.js",
  "contributes": {
    "configuration": {
      "title": "Kanban",
      "properties": {
        "kanban.statusButton.alignment": {
          "type": "string",
          "default": "Left",
          "enum": [
            "Left",
            "Right",
            "None"
          ],
          "description": "Show the Kanban button on the left or right side of your status bar, or nowhere."
        },
        "kanban.statusButton.priority": {
          "type": "number",
          "default": 100,
          "description": "Where the Kanban button should be in relation to other buttons. A higher value means further left."
        },
        "kanban.saveFiles.pathPreferences": {
          "type": "array",
          "default": [
            ".vscode/kanban.json",
            "kanban.json"
          ],
          "description": "When attempting to load from a file, check these paths from left to right, stopping at first success."
        }
      }
    },
    "commands": [
      {
        "command": "kanban.view",
        "title": "Kanban: View"
      }
    ]
  },
  "repository": "https://github.com/lbauskar/kanban.git",
  "homepage": "/",
  "scripts": {
    "vscode:prepublish": "(webpack --config webpack.config.js --mode=production) && (webpack --config webpack-extension.config.js --mode=production)",
    "build": "(webpack --config webpack.config.js --mode=development) && (webpack --config webpack-extension.config.js --mode=development)",
    "test": "jest --env=jsdom",
    "package": "vsce package"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^12.11.7",
    "@types/react": "^17.0.15",
    "@types/react-beautiful-dnd": "^13.1.1",
    "@types/react-dom": "^17.0.9",
    "@types/uuid": "^8.3.1",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vscode/test-electron": "^2.1.3",
    "css-loader": "^6.7.1",
    "eslint": "^7.19.0",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.6.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.1.3 < 4.5.0",
    "vsce": "^2.7.0",
    "vscode-test": "^1.5.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "@szhsin/react-menu": "^2.1.0",
    "@vscode/codicons": "^0.0.21",
    "csstype": "^3.0.11",
    "cuid": "^2.1.8",
    "just-clone": "^3.2.1",
    "path-browserify": "^1.0.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-colorful": "^5.3.0",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.1.0",
    "react-markdown": "^6.0.3",
    "react-textarea-autosize": "^8.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
