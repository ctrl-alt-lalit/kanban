{
  "publisher": "lbauskar",
  "name": "kanban",
  "displayName": "Kanban",
  "description": "Simple kanban board for VS Code. Visually organize your ideas!",
  "icon": "icon.png",
  "version": "1.1.2",
  "engines": {
    "vscode": "^1.53.2"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "categories": [
    "Visualization",
    "Notebooks"
  ],
  "keywords": [
    "kanban",
    "board",
    "agile",
    "scrum",
    "sprint"
  ],
  "extensionKind": [
    "ui"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./build/extension.js",
  "contributes": {
    "configuration": {
      "title": "Kanban",
      "properties": {
        "kanban.showViewButton": {
          "type": "boolean",
          "default": true,
          "description": "Add a button to the bottom left of your status bar to view the kanban board. You can still view the board in the Command Pallete with 'Kanban: View'"
        }
      }
    },
    "commands": [
      {
        "command": "kanban.view",
        "title": "Kanban: View"
      }
    ]
  },
  "repository": "https://github.com/lbauskar/kanban.git",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "start": "react-scripts start",
    "build": "node ./build-non-split.js && tsc -p tsconfig.extension.json",
    "eject": "react-scripts eject",
    "test": "jest"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/react": "^17.0.15",
    "@types/react-beautiful-dnd": "^13.1.1",
    "@types/react-dom": "^17.0.9",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "mocha": "^8.2.1",
    "react-scripts": "^4.0.3",
    "rewire": "^5.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.21",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-colorful": "^5.3.0",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.1.0",
    "react-markdown": "^6.0.3",
    "react-textarea-autosize": "^8.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
